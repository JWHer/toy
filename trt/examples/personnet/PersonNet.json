{
    "task": "RetinaNet",
    "model": {
        "backbone":{
            "name":"resnet18",
            "out_indices": [2, 3, 4]
        },
        "neck": {
            "name":"FeaturePyramidNetwork",
            "in_channels": [128, 256, 512],
            "out_channels": 256
        },
        "head": {
            "name": "RetinaNetHead",
            "in_channels": 256, 
            "aspect_ratio": [0.5, 1.0, 2.0],
            "anchor_size": [12, 48, 96, 192, 384],
            "topk_candidates": 100,
            "fg_iou_thresh": 0.5, 
            "bg_iou_thresh":0.4,
            "focal_loss_cfg": {
                "alpha": 0.25,
                "gamma": 3
            }
        }
    },
    "detections_per_img": 100,
    "nms_thresh": 0.5,
    "min_score": 0.05,
    "optim": {
        "name": "SGD",
        "lr": 0.01,
        "momentum": 0.9,
        "weight_decay": 0.0005

    },  
    "lr_cfg": {
        "type": "step",
        "steps": [16, 22],
        "decay": 0.1
    },
    "use_model_ema": false,
    "max_epoch":24,
    "seed": null,
    "classes": ["person"],
    "num_classes": 1    
}