{ 
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": { 
        "standard": { 
            "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
        }
    },
    "handlers": { 
        "default": { 
            "level": "DEBUG",
            "formatter": "standard",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout"
        },
        "file": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "INFO",
            "formatter": "standard",
            "filename": "resource/web.log",
            "mode": "a",
            "maxBytes": 10485760,
            "backupCount": 10
        }
    },
    "loggers": { 
        "": {  
            "handlers": ["default", "file"],
            "level": "DEBUG",
            "propagate": false
        }
    } 
}